    is_singleton [12.0,12.0] = true;
    is_singleton [17.1, 17.1] = true;
    is_singleton [-0.0,-0.0] = true;
    is_singleton [0.0,0.0] = true;
    is_singleton [-0.0, 0.0] = true;
    is_singleton [0.0, -0.0] = true;

    is_singleton [empty] = false;
    is_singleton [entire] = false;
    is_singleton [-1.0, 0.0] = false;
    is_singleton [-1.0, -0.5] = false;
    is_singleton [1.0, 2.0] = false;
    is_singleton [-inf,-0x1.FFFFFFFFFFFFFp1023] = false;
    is_singleton [-1.0,inf] = false;
}

