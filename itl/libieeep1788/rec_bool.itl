testcase minimal_is_common_test {
    is_common [-27.0,-27.0] = true;
    is_common [-27.0, 0.0] = true;
    is_common [0.0,0.0] = true;
    is_common [-0.0,-0.0] = true;
    is_common [-0.0,0.0] = true;
    is_common [0.0,-0.0] = true;
    is_common [5.0, 12.4] = true;
    is_common [-0x1.FFFFFFFFFFFFFp1023, 0x1.FFFFFFFFFFFFFp1023] = true;

    is_common [entire] = false;
    is_common [empty] = false;
    is_common [-inf, 0.0] = false;
    is_common [0.0, inf] = false;
}

